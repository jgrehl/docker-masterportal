{
    "plugins": [
      "node_modules/jsdoc-vuejs"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["./devtools/jsdoc", "./js", "./src_3_0_0", "./addons"],
        "includePattern": "\\.(vue|js)$",
        "excludePattern": "(node_modules\\/)|(addons|node_modules\\/)|(addons(\\/|\\\\).*(\\/|\\\\)node_modules(\\/|\\\\))|((^|\\/|\\\\)__)"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "linenums": true,
    "opts": {
      "encoding": "utf8",
      "recurse": true,
      "destination": "./jsdoc"
    }
}
