    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-polyfill": "6.26.0",
    "babel-jest": "^27.0.6",
    "canvas": "^2.8.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "jest-canvas-mock": "^2.3.1",
    "jsdoc": "^3.6.7",
    "parcel-bundler": "^1.12.5",
